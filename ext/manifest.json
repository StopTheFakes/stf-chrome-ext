{
	"name": "STF",
	"version": "0.1",
	"description": "STF extension",
	"permissions": ["tabs", "<all_urls>", "unlimitedStorage", "storage"],
	"browser_action": {
		"default_title": "STF",
		"default_icon": "icon.png",
		"default_popup": "index.html"
	},
	"manifest_version": 2,
	"content_security_policy": "script-src 'self'; object-src 'self'"
}
